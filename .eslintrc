{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
    	"no-undef": 2,
	    "no-unused-vars": 2
    },

    "overrides": [
        {
          "files": [
            "**/*.mjs"
          ],
          "parserOptions": {
            "sourceType": "module"
          }
        }
      ]
}